<h1>Listing rounds</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rounds.each do |round| %>
  <tr>
    <td><%= link_to 'Show', fight_round_path(@fight, round) %></td>
    <td><%= link_to 'Edit', edit_fight_round_path(@fight, round) %></td>
    <td><%= link_to 'Destroy', fight_round_path(@fight, round), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

